{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public",
        "cleanDistDir": true
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/client/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/public/index.html"
    }
  ]
}
